@echo off
setlocal enabledelayedexpansion

:: Directory path (replace with your target directory)
set "target_dir=C:\Users\Wei\Desktop\bible-verse\static\horoscope-images"

:: Change to the target directory
cd /d "%target_dir%"

:: Counter to track file numbering
set /a count=1

:: Loop through all files in the directory
for %%f in (*) do (
    :: Get the file extension
    set "filename=%%~nf"
    set "extension=%%~xf"
    
    :: Rename the file with an incrementing number
    ren "%%f" "!count!%%extension!"
    
    :: Increment the counter
    set /a count+=1
)

echo All files have been renamed.
pause
